{
  "name": "reverse-array-new",
  "version": "1.0.0",
  "description": "Drop in replacement for reverse-array npm package",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "watch": "npm run build -- --watch src",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ravinder-Olivier/reverse-array-new.git"
  },
  "author": "Ravinder Olivier Dadiala <ravinder-Olivier@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ravinder-Olivier/reverse-array-new/issues"
  },
  "homepage": "https://github.com/ravinder-Olivier/reverse-array-new#readme",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.4",
    "@types/jest": "^29.5.4",
    "jest": "^29.6.4",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
